pages = {755},
title = {{The Importance of Institutional Design for Distributed Local-Level Governance of Groundwater: The Case of California's Sustainable Groundwater Management Act}},
url = {http://www.mdpi.com/2073-4441/9/10/755},
volume = {9},
year = {2017}
}
