file = {:home/julian/Documents/Mendeley Desktop/Bernhardt et al/Bernhardt et al.{\_}2007.pdf:pdf},
issn = {1061-2971},
journal = {Restoration Ecology},
keywords = {Evaluation,Interviews,Monitoring,NRRSS,River restoration,Success},
month = {sep},
