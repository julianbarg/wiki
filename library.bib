pmid = {33534822},
title = {{Swimming with Sharks: Technology Ventures, Defense Mechanisms and Corporate Relationships}},
url = {http://journals.sagepub.com/doi/10.2189/asqu.53.2.295},
volume = {53},
year = {2008}
}
