doi = {10.1177/0003122417728240},
file = {:home/julian/Documents/Mendeley Desktop/Fligstein, Stuart Brundage, Schultz/Fligstein, Stuart Brundage, Schultz{\_}2017.pdf:pdf},
isbn = {0003122417},
issn = {0003-1224},
journal = {American Sociological Review},
keywords = {culture,economic sociology,finance,organizations,policymaking},
month = {oct},
number = {5},
pages = {879--909},
title = {{Seeing Like the Fed: Culture, Cognition, and Framing in the Failure to Anticipate the Financial Crisis of 2008}},
