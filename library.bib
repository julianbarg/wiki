journal = {Organization Science},
keywords = {ambiguity,aspiration,learning},
month = {nov},
number = {November},
pages = {orsc.2020.1383},
title = {{The Plural of Goal: Learning in a World of Ambiguity}},
url = {papers3://publication/uuid/C35B8935-50B7-4387-AFD9-0484E6BFFEE8 http://pubsonline.informs.org/doi/10.1287/orsc.2020.1383},
year = {2020}
}
