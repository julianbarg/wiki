@article{Sullivan2007,
abstract = {In this study, we examine the effects of illegal/unethical acts on$\backslash$ninterfirm networks. We hypothesize that the quality of network partners$\backslash$nwill decline and overall network structure will change after a firm$\backslash$ncommits an unethical act. More specifically, we posit that the decline$\backslash$nin partner quality is primarily driven by higher quality firms leaving$\backslash$nthe network, and the focal firm being forced to replace them with lower$\backslash$nquality ones. We also propose that partner prominence and network$\backslash$ncohesion will be affected after these acts, and that the changes in$\backslash$npartner quality and network structure will be greater for those acts$\backslash$nperceived as more illegitimate. We test these hypotheses using a sample$\backslash$nof 200 large firms in the United States and data on unethical acts by$\backslash$nthese firms. Our results show that the quality of a firm's network$\backslash$npartners declines after the firm's commission of an unethical act, and$\backslash$nthat the deterioration in partner quality tends to be greater for acts$\backslash$nof greater illegitimacy. Our results also show declines in network$\backslash$nprominence and cohesion for firms involved in these activities. We$\backslash$ndiscuss the implications of our results for the literatures on$\backslash$ninterfirm networks and unethical corporate activities.},
author = {Sullivan, Bilian Ni and Haunschild, Pamela and Page, Karen},
doi = {10.1287/orsc.1060.0229},
file = {:home/julian/Documents/Mendeley Desktop/Sullivan, Haunschild, Page/Sullivan, Haunschild, Page{\_}2007.pdf:pdf},
