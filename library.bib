isbn = {1047-7039},
issn = {1047-7039},
journal = {Organization Science},
keywords = {2014,censure episodes,control,ethnography,expertise,history,in advance june 6,knowledge work,labor process,management systems,organizational change,process theory,published online in articles,resistance,status degradation,work},
number = {6},
