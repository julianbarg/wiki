pages = {1994--2020},
title = {{Not All Inequality Is Equal: Deconstructing the Societal Logic of Patriarchy to Understand Microfinance Lending to Women}},
url = {http://amj.aom.org/cgi/doi/10.5465/amj.2015.0476},
volume = {59},
year = {2016}
}
@unpublished{Xu2020a,
annote = {I would assume that serial entrepreneurs simply hire the best people available--how many applicants are there usually anyways? Or do they typically hire through their network?

