@article{Huising2014,
abstract = {Organizations depend on experts to oversee and execute complex tasks.$\backslash$nWhen faced with pressures to reduce their dependence on experts,$\backslash$nmanagers encounter a control paradox: they require experts to explicate$\backslash$nthe very knowledge and discretionary approaches that are the basis$\backslash$nof their control for the purpose of undercutting this control. Experts$\backslash$nrarely consent to such a situation; therefore, attempts to reduce$\backslash$ndependence on experts and control their work are more often aspirational$\backslash$nthan actual. Drawing on an ethnography of an organization that was$\backslash$nrequired by a government agency to transfer the work responsibilities$\backslash$nof experts to employees throughout the organization, this paper describes$\backslash$nhow a network of actors developed a discursive, political process$\backslash$nto renegotiate control of expert work practices. Through censure$\backslash$nepisodes, long-standing and largely successful expert practices were$\backslash$nexamined one by one and relabeled as problematic in relation to established$\backslash$ngoals. The constructed breaches opened expert practices to evaluation,$\backslash$nquestioning, and eventual delegitimation within the organization.$\backslash$nThis process depended on the introduction of new roles that revised$\backslash$ndependencies and generated new resources. This paper contributes$\backslash$nto the understanding of control in organizations by theorizing how$\backslash$nthe emergent, symbolic work of censure episodes are a means of gradually$\backslash$nsubverting expert control. Further, these struggles are reconceptualized$\backslash$nas multiple-role negotiations rather than bilateral manager{\^{a}}expert$\backslash$nstruggles.},
author = {Huising, Ruthanne},
