journal = {Journal of Economic Perspectives},
number = {1},
pages = {3--7},
title = {{What is Economic Sociology and Should any Economists Care?}},
url = {http://pubs.aeaweb.org/doi/10.1257/0895330053147912},
volume = {19},
year = {2005}
}
