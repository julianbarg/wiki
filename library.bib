journal = {Strategic Management Journal},
keywords = {attention,environmental change,noticing,opportunity belief,strategic action},
month = {mar},
number = {3},
pages = {626--644},
title = {{Is that an Opportunity? An Attention Model of Top Managers' Opportunity Beliefs for Strategic Action}},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/smj.2499},
volume = {38},
year = {2017}
