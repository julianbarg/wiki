keywords = {CSR managers,corporate social responsibility (CSR),institutionalization,organizational professionals,professionalization},
number = {5},
pages = {613--646},
title = {{Reconsidering the ‘Symmetry' Between Institutionalization and Professionalization: The Case of Corporate Social Responsibility Managers}},
volume = {54},
year = {2017}
}
