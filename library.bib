issn = {00018392},
journal = {Administrative Science Quarterly},
keywords = {drug courts,ethnography,institutional complexity,institutional logics,professions},
number = {2},
pages = {165--196},
pmid = {87803840},
title = {{Logics in Action: Managing Institutional Complexity in a Drug Court}},
volume = {58},
year = {2013}
}
