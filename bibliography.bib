@article{Yan2019,
  title = {The {{Rise}} of {{Socially Responsible Investment Funds}}: The {{Paradoxical Role}} of the {{Financial Logic}}},
  shorttitle = {The {{Rise}} of {{Socially Responsible Investment Funds}}},
  author = {Yan, Shipeng and Ferraro, Fabrizio and Almandoz, Juan (John)},
  date = {2019-06-01},
