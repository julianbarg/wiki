@article{Zhong2021,
  title = {What {{Happens}} to {{Bad Actors}} in {{Organizations}}? A {{Review}} of {{Actor}}-{{Centric Outcomes}} of {{Negative Behavior}}},
  shorttitle = {What {{Happens}} to {{Bad Actors}} in {{Organizations}}?},
  author = {Zhong, Rui and Robinson, Sandra L.},
  date = {2021-07-01},
