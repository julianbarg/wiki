  shorttitle = {Interactions between {{Zooplankton}} and {{Crude Oil}}},
  author = {Almeda, Rodrigo and Wambaugh, Zoe and Wang, Zucheng and Hyatt, Cammie and Liu, Zhanfei and Buskey, Edward J.},
